{
  "name": "budget-app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "main": "electron.cjs",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "electron": "electron .",
    "electron-dev": "NODE_ENV=development electron .",
    "build-electron": "vite build && electron-builder",
    "dist": "electron-builder"
  },
  "build": {
    "appId": "com.budgetapp.app",
    "productName": "Budget App",
    "files": [
      "dist/**/*",
      "electron.cjs",
      "preload.cjs",
      "src/db/**/*"
    ],
    "asarUnpack": [
      "dist/**/*"
    ],
    "directories": {
      "output": "release"
    },
    "mac": {
      "category": "public.app-category.finance",
      "target": "dmg"
    }
  },
  "dependencies": {
    "@types/better-sqlite3": "^7.6.13",
    "better-sqlite3": "^12.4.1",
    "chart.js": "^4.5.0",
    "pinia": "^3.0.3",
    "vue": "^3.5.21",
    "vue-chartjs": "^5.3.2"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.13",
    "@vitejs/plugin-vue": "^6.0.1",
    "autoprefixer": "^10.4.21",
    "electron": "^38.2.0",
    "electron-builder": "^26.0.12",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.17",
    "vite": "^7.1.7"
  }
}
